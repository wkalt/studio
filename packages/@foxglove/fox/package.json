    "prepublishOnly": "tsc && yarn copy-fox && yarn test",
    "fox": "node dist/fox"
  },
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@types/copyfiles": "^2",
    "@types/node": "^12",
    "copyfiles": "2.4.1",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "commander": "7.2.0"
  }
}
