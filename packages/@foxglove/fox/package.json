{
  "name": "@foxglove/fox",
  "version": "0.1.0",
  "description": "Foxglove Studio Extension Manager",
  "license": "MPL-2.0",
  "main": "dist/api.js",
  "typings": "dist/api.d.ts",
  "bin": "dist/fox",
  "scripts": {
    "copy-fox": "copyfiles -f src/fox dist",
    "compile": "tsc && yarn copy-fox",
